{
    "name": "mdb-reader",
    "version": "2.0.0-next.4",
    "description": "JavaScript library to read data from Access databases",
    "main": "lib/cjs/index.js",
    "module": "lib/esm/index.js",
    "types": "lib/types/index.d.ts",
    "exports": {
        ".": {
            "require": "./lib/cjs/index.js",
            "default": "./lib/esm/index.js"
        }
    },
    "sideEffects": false,
    "files": [
        "lib"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/andipaetzold/mdb-reader.git"
    },
    "keywords": [
        "mdb",
        "microsoft",
        "access",
        "database"
    ],
    "author": {
        "name": "Andi PÃ¤tzold",
        "email": "github@andipaetzold.com",
        "url": "https://github.com/andipaetzold"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/andipaetzold/mdb-reader/issues"
    },
    "homepage": "https://github.com/andipaetzold/mdb-reader#readme",
    "devDependencies": {
        "@semantic-release/changelog": "5.0.1",
        "@semantic-release/git": "9.0.1",
        "@types/jest": "27.0.2",
        "@types/node": "14.14.37",
        "jest": "27.2.5",
        "rimraf": "3.0.2",
        "semantic-release": "17.4.7",
        "ts-jest": "27.0.5",
        "tslib": "2.3.1",
        "typescript": "4.4.3"
    },
    "scripts": {
        "prepublishOnly": "npm run build",
        "build": "rimraf lib && npm run build:cjs && npm run build:esm",
        "build:cjs": "rimraf lib/cjs && tsc --module commonjs --outDir lib/cjs",
        "build:esm": "rimraf lib/esm && tsc --module ES2015 --outDir lib/esm",
        "test": "jest",
        "semantic-release": "semantic-release"
    }
}
